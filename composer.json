{
    "name": "vaghey/image-editor",
    "description": "GD-powered image manipulation for Laravel",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Vaghey\\ImageEditor\\": "src/"
        },
        "files": [
            "src/Helpers/ImageEditor.php"
        ]
    },
    "authors": [
        {
            "name": "Ali Vaghey"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "illuminate/support": "^12.31",
        "ext-gd": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vaghey\\ImageEditor\\ImageEditorServiceProvider"
            ]
        }
    }
}
